{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "povbackend",
      "image": "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_DEFAULT_REGION}.amazonaws.com/povbackend:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ]
    }
  ]
}