{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
      {
        "name": "povstreaming",
        "image": "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_DEFAULT_REGION}.amazonaws.com/povstreaming:latest",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          },
          {
            "hostPort": 443,
            "containerPort": 443
          },
          {
            "hostPort": 1935,
            "containerPort": 1935,
            "bindIP": "0.0.0.0"
          }
        ]
      }
    ]
  }
  